{
  "name": "api",
  "version": "0.0.1",
  "packageManager": "pnpm@9.12.3",
  "main": "index.js",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "pnpm prisma:generate && tsc --build tsconfig.build.json && tsc-alias -p tsconfig.json",
    "start": "pnpm prodEnv cross-env NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist node_modules",
    "devEnv": "dotenv -e .env.development.local --",
    "prodEnv": "dotenv -e .env.production.local --",
    "prisma:generate": "pnpm devEnv npx prisma generate",
    "prisma:db-push": "pnpm devEnv npx prisma db push",
    "prisma:migrate": "pnpm devEnv npx prisma migrate dev --name init",
    "prisma:seed": "pnpm devEnv npx prisma db seed",
    "prisma:studio": "pnpm devEnv npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "@vani0-0/logger": "workspace:*",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "nocache": "^4.0.0",
    "path": "^0.12.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@swc/core": "^1.8.0",
    "@swc/helpers": "^0.5.12",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.8.7",
    "@types/nodemailer": "^6.4.15",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4"
  }
}
