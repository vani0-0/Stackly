    "db:push": "prisma db push --skip-generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@prisma/extension-accelerate": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "prisma": "^5.21.1",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  }
}
